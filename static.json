{
    "headers": {
        "/**": {
            "Content-Type": "text/plain; charset=UTF-8 ",
            "Access-Control-Allow-Origin": "*"
        },
        "/fetch/**": {
            "Content-Type": "text/html; charset=UTF-8 ",
            "Access-Control-Allow-Origin": "*"
        }
    },
    "routes": {
        "/login/": "login.html",
        "/sample/": "sample.html",
        "/promise/": "promise.html",
        "/fetch/": "fetch.html"
    }
}